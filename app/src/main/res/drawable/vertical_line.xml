<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">
    <shape
        android:shape="line">
        <size
            android:width="15dp"
            android:height="15dp" />
        <stroke
            android:width="5dp"
            android:color="@color/colorGreen"
            android:dashGap="2dp" />
    </shape>
</rotate>